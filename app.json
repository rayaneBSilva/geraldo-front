{
  "expo": {
    "name": "geraldo-front",
    "slug": "geraldo-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/geraldo-login.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/tela_de_carregamento.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/geraldo-login.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/geraldo-login.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
